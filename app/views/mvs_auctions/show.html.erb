<%- model_class = MvsAuction -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:mvs_game_id) %>:</strong></dt>
  <dd><%= @mvs_auction.mvs_game_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:item_id) %>:</strong></dt>
  <dd><%= @mvs_auction.item_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:title) %>:</strong></dt>
  <dd><%= @mvs_auction.title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:url) %>:</strong></dt>
  <dd><%= @mvs_auction.url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:thumb) %>:</strong></dt>
  <dd><%= @mvs_auction.thumb %></dd>
  <dt><strong><%= model_class.human_attribute_name(:auction_end) %>:</strong></dt>
  <dd><%= @mvs_auction.auction_end %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ended_at) %>:</strong></dt>
  <dd><%= @mvs_auction.ended_at %></dd>
  <dt><strong><%= model_class.human_attribute_name(:price) %>:</strong></dt>
  <dd><%= @mvs_auction.price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:approved) %>:</strong></dt>
  <dd><%= @mvs_auction.approved %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              mvs_auctions_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_mvs_auction_path(@mvs_auction), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mvs_auction_path(@mvs_auction),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
